sentence:
  : data-literal*
  ;
data-literal:
  : list
  : tuple
  : map
  : primitive
  ;
primitive:
  : integer
  : atom
  : boolean
  ;
list:
  : '[]'
  : '[' ( data-literal ',' )* data-literal ']'
  ;
tuple:
  : '()'
  : '(' ( data-literal ',' )* data-literal ')'
  ;
map:
  : '%{}'
  : '%{' ( key-pair ',' )* key-pair '}'
  ;
key-pair:
  : data-literal '=>' data-literal
  : key data-literal
  ;
integer:
  : INTEGER
  ; 
atom:
  : ATOM
  ;
key:
  ; KEY
  ;
boolean:
  : 'true'
  : 'false'
  ;
